<script type="text/javascript" src="../jquery/plugins/countdown/jquery.countdown.js"></script>


<script>
window.onload = function () {
var marathonDate = new Date(2014, 10, 2,10, 40, 0);

$('#countdown').appendTo('#ft_header');
 $('#countdown').countdown({until: 
    $.countdown.UTCDate(-5, marathonDate), layout: '<div id="fredsteam-title" class="fredsteam-col">Countdown to the<br><span class="mlabel fredsteam-bold">TCS NYC MARATHON</span></div><div class="fredsteam-col"><span class="fredsteam-num fredsteam-bold">{dn}</span><span id="days" class="fredsteam-label">Days</span></div><div class="fredsteam-col"><span class="fredsteam-num fredsteam-bold">{hnn}</span><span id="hours" class="fredsteam-label">Hours</span></div><div class="fredsteam-col fredsteam-last-col"><span class="fredsteam-num fredsteam-bold">{mnn}</span><span id="minutes" class="fredsteam-label  fredsteam-noborder">Minutes</span></div>'})
};

for(var i = 14; i > 6; i--){

  $('.scrollContent p').eq(i).prependTo('#topFundraisersTwo');
}

</script>